	ori $8, $8, 0x26A8
	lui $9, 0x1001
	ori $9, $9, 0x2724 #endereço final linha 1

	lui $10, 0x1001 #endereço começo linha 2
	ori $10, $10, 0x28A8
	lui $11, 0x1001
	ori $11, $11, 0x2924 #endereço final linha 2
	
	lui $12, 0x1001 #endereço começo linha 3
