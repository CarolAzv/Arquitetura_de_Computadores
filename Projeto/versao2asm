	ori $23, $23, 0x34F0 #endereço final linha 8
	
	lui $24, 0x1001 #endereço começo linha 9
	ori $24, $24, 0x36A8
	lui $25, 0x1001
	ori $25, $25, 0x36F0 #endereço final linha 9
	
	lui $26, 0x00ff #cor branca das nuvens
